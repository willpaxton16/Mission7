@{
}

@model IEnumerable<string>

<div class="gap-2">
    <a asp-controller="Home" asp-action="Index" asp-route-category=""
       asp-route-pageNum="1" class="btn btn-outline-secondary">Home</a>
    <br />

    @foreach (string t in Model)
    {
        <a asp-controller="Home" asp-action="Index" asp-route-category="@t" asp-route-pageNum="1"
           class="btn @(ViewBag.SelectedType == t ? "btn-primary" : "btn-secondary")">@t</a>
        <br />
    }
</div>
